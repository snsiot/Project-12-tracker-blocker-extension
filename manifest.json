{
  "manifest_version": 3,
  "name": "Tracker Blocker",
  "version": "1.0",
  "description": "Blocks known tracking scripts and shows blocked count.",
  "permissions": ["webRequest", "webRequestBlocking", "storage"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Trackers Blocked"
  },
  "icons": {
    "48": "icons/icon.png"
  }
}
